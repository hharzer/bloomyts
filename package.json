  },
  "homepage": "https://github.com/hharzer/bloomyts#readme",
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.6",
    "ajv": "^6.12.0",
    "ajv-sanitizer": "^1.1.4",
    "async-hooks": "^1.3.1",
    "bluebird": "^3.7.2",
    "dotenv": "^8.2.0",
    "faunadb": "^2.11.1",
    "faunadb-fql-lib": "^0.9.0",
    "hooks": "^0.3.2",
    "json-schema-shorthand": "^0.3.2",
    "lodash": "^4.17.15"
