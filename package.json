{
  "name": "bloomyts",
  "version": "0.0.2d",
  "license": "MIT",
  "devDependencies": {
    "eslint": "6.8.0",
    "prettier": "1.19.1",
    "tslint": "6.0.0",
    "tslint-config-prettier": "1.18.0",
    "typedoc": "0.16.10",
    "typescript": "3.8.2"
  },
  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\"",
